@model IEnumerable<Onym.Models.User>
@{
    ViewBag.Title = "UserList";
    ViewBag.Page = "UserManage";
}
<h2>Список пользователей</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="EditUserRoles" asp-controller="Admin" asp-route-userid="@user.Id">Права доступа</a>
            </td>
        </tr>
    }
</table>